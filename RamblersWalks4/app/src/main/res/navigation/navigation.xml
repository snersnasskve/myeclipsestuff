<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/navigation"
    xmlns:tools="http://schemas.android.com/tools"

    app:startDestination="@+id/mainFragment4">


    <fragment
        android:id="@+id/mainFragment4"
        android:name="com.sners.ramblerswalks4.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_mainFragment4_to_groupsFragment"
            app:destination="@id/groupsFragment" />
        <action
            android:id="@+id/action_mainFragment4_to_distanceFragment"
            app:destination="@id/distanceFragment" />
        <action
            android:id="@+id/action_mainFragment4_to_daysFragment"
            app:destination="@id/daysFragment" />
    </fragment>
    <fragment
        android:id="@+id/groupsFragment"
        android:name="com.sners.ramblerswalks4.GroupsFragment"
        android:label="fragment_groups"
        tools:layout="@layout/fragment_groups" />
    <fragment
        android:id="@+id/distanceFragment"
        android:name="com.sners.ramblerswalks4.DistanceFragment"
        android:label="fragment_distance"
        tools:layout="@layout/fragment_distance" />
    <fragment
        android:id="@+id/daysFragment"
        android:name="com.sners.ramblerswalks4.DaysFragment"
        android:label="fragment_days"
        tools:layout="@layout/fragment_days" />
</navigation>